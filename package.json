{
    "author": "Jacob K.F. Bogers",
    "main": "build/lib/index.js",
    "browser": "dist/blasjs.min.js",
    "types": "build/lib/index.d.ts",
    "version": "1.0.13",
    "files": [
        "dist",
        "build"
    ],
    "bugs": {
        "url": "https://github.com/jacobbogers/blasjs/issues"
    },
    "contributes": {
        "typescriptServerPlugins": [
            {
                "name": "typescript-eslint-language-service"
            }
        ]
    },
    "dependencies": {
        "ts-node": "^9.0.0",
        "typescript": "^4.1.2"
    },
    "description": "Javascript Pure Implementation of BLAS Level 1, level 2, level 3 functions",
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^10.0.0",
        "@types/chai": "^3.5.2",
        "@types/mocha": "^2.2.48",
        "@types/node": "^6.0.96",
        "@typescript-eslint/eslint-plugin": "^4.8.1",
        "@typescript-eslint/parser": "^4.8.1",
        "chai": "^3.5.0",
        "eslint": "^7.14.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "^26.6.3",
        "nyc": "^11.5.0",
        "prettier": "^2.2.0",
        "rollup": "^2.33.1",
        "rollup-plugin-terser": "^7.0.2",
        "schema-utils": "^0.4.5",
        "source-map-support": "^0.5.3",
        "tslib": "^2.0.3",
        "typescript-eslint-language-service": "^4.1.1"
    },
    "homepage": "https://github.com/jacobbogers/blasjs#readme",
    "keywords": [
        "blas",
        "linear algebra",
        "lapack",
        "octave",
        "linear regression",
        "vector algebra",
        "generalized linear model",
        "matrix",
        "vector",
        "complex matrix",
        "conjugate matrix",
        "transpose matrix",
        "matrix inverse",
        "determinant",
        "jacobian",
        "Hermitian",
        "conjugate",
        "conjugate matrix"
    ],
    "license": "GPL-3.0",
    "name": "blasjs",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jacobbogers/blasjs.git"
    },
    "scripts": {
        "bnew": "tsc",
        "build": "webpack --progress --color --display-error-details --config config/webpack/config",
        "build:prod": "webpack --progress --color --display-error-details --config config/webpack/config --env=prod",
        "lint": "eslint 'src/**/*.ts' --fix",
        "lint:report": "npm run lint -- -o .lint-report.txt --force && code .lint-report.txt",
        "post-test": "echo posttest",
        "prepublishOnly": "tsc && npm run build:prod",
        "report": "opn coverage/lcov-report/index.html",
        "test": "cross-env-shell NODE_ENV=test TS_NODE_DISABLE_WARNINGS=true nyc mocha"
    }
}
